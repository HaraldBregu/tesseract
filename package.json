  "scripts": {
    "old-dev": "npm-run-all --parallel dev-react dev-electron",
    "dev": "electron-vite dev",
    "dev-react": "vite",
    "dev-electron": "npm run transpile-electron && cross-env NODE_ENV=development electron .",
    "old-build": "tsc -b && vite build",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "build": "npm run typecheck && electron-vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "transpile-electron": "tsc --project src/main/tsconfig.json",
    "svgr": "node svgr-script.cjs",
    "pack-win": "electron-builder --win --x64",
    "pack-mac-intel": "electron-builder --mac --x64",
    "pack-mac-arm": "electron-builder --mac --arm64",
    "pack-linux": "electron-builder --linux --x64",
    "dist-win": "npm run build && electron-builder --win --x64",
    "dist-mac-intel": "npm run build && electron-builder --mac --x64",
    "dist-mac-arm": "npm run build && electron-builder --mac --arm64",
    "dist-linux": "npm run build && electron-builder --linux --x64",
    "build:electron": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "clean": "rimraf dist out && rimraf node_modules/.cache/electron && rimraf node_modules/.cache/electron-builder",
    "generate-icons": "electron-icon-builder --input=buildResources/appIcons/icon_512.png --output=./buildResources",
    "test": "jest"
  },
