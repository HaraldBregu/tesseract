  "main": "./out/main/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron-vite dev --inspect",
    "dev:staging": "cross-env NODE_ENV=staging electron-vite dev --inspect --mode staging",
    "dev:prod": "cross-env NODE_ENV=production electron-vite dev --inspect --mode production",
    "dev-linux": "cross-env NODE_ENV=development ELECTRON_DISABLE_SANDBOX=1 electron-vite dev",
    "dev-linux:staging": "cross-env NODE_ENV=staging ELECTRON_DISABLE_SANDBOX=1 electron-vite dev --mode staging",
    "dev-linux:prod": "cross-env NODE_ENV=production ELECTRON_DISABLE_SANDBOX=1 electron-vite dev --mode production",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "build": "cross-env NODE_ENV=production npm run typecheck && electron-vite build",
    "build:dev": "cross-env NODE_ENV=development npm run typecheck && electron-vite build --mode development",
    "build:staging": "cross-env NODE_ENV=staging npm run typecheck && electron-vite build --mode staging",
    "lint": "eslint .",
    "dist-all": "npm run dist-win && npm run dist-linux && npm run dist-mac",
    "dist-all:dev": "npm run dist-win:dev && npm run dist-linux:dev && npm run dist-mac:dev",
    "dist-all:staging": "npm run dist-win:staging && npm run dist-linux:staging && npm run dist-mac:staging",
    "dist-win": "cross-env NODE_ENV=production npm run build && electron-builder --win --x64 && node scripts/package-build.cjs windows pre",
    "dist-win:dev": "cross-env NODE_ENV=development npm run build:dev && electron-builder --win --x64 && node scripts/package-build.cjs windows dev",
    "dist-win:staging": "cross-env NODE_ENV=staging npm run build:staging && electron-builder --win --x64 && node scripts/package-build.cjs windows stg",
    "dist-linux": "cross-env NODE_ENV=production npm run build && electron-builder --linux --x64 --config.linux.target=deb && node scripts/package-build.cjs linux pre",
    "dist-linux:dev": "cross-env NODE_ENV=development npm run build:dev && electron-builder --linux --x64 --config.linux.target=deb && node scripts/package-build.cjs linux dev",
    "dist-linux:staging": "cross-env NODE_ENV=staging npm run build:staging && electron-builder --linux --x64 --config.linux.target=deb && node scripts/package-build.cjs linux stg",
    "dist-mac": "cross-env NODE_ENV=production ./scripts/fix-mac-build.sh && npm run build && electron-builder --mac pkg --x64 && electron-builder --mac pkg --arm64 && node scripts/package-build.cjs macos pre",
    "dist-mac:dev": "cross-env NODE_ENV=development ./scripts/fix-mac-build.sh && npm run build:dev && electron-builder --mac pkg --x64 && electron-builder --mac pkg --arm64 && node scripts/package-build.cjs macos dev",
    "dist-mac:staging": "cross-env NODE_ENV=staging ./scripts/fix-mac-build.sh && npm run build:staging && electron-builder --mac pkg --x64 && electron-builder --mac pkg --arm64 && node scripts/package-build.cjs macos stg",
    "postinstall": "electron-builder install-app-deps",
    "clean": "rimraf dist out && rimraf node_modules/.cache/electron && rimraf node_modules/.cache/electron-builder",
    "test": "jest",
    "svgr": "node svgr-script.cjs",
    "generate-icons": "electron-icon-builder --input=buildResources/appIcons/icon_512.png --output=./buildResources",
    "shadcn": "shadcn-ui"
  },
