  "private": true,
  "version": "0.3.0",
  "type": "module",
  "main": "./out/main/index.js",
  "scripts": {
    "old-dev": "npm-run-all --parallel dev-react dev-electron",
    "dev": "electron-vite dev",
    "dev-react": "vite",
    "dev-electron": "npm run transpile-electron && cross-env NODE_ENV=development electron .",
    "old-build": "tsc -b && vite build",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "build": "npm run typecheck && electron-vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "transpile-electron": "tsc --project src/main/tsconfig.json",
    "svgr": "node svgr-script.cjs",
    "old-dist-win": "npm run transpile-electron && npm run build && npx electron-builder --win --x64",
    "old-dist-mac": "npm run transpile-electron && npm run build && npx electron-builder --mac --arm64",
    "old-dist-linux": "npm run transpile-electron && npm run build && npx electron-builder --linux --x64",
    "dist-win": "npm run build && electron-builder --win --x64",
    "dist-mac": "npm run build && electron-builder --mac --arm64",
    "dist-linux": "npm run build && electron-builder --linux --x64",
    "build:electron": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build:sec && electron-builder --win --x64",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "clean": "rimraf dist out"
  },
